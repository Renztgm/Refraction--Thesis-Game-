[sub_resource type="GDScript" id="GDScript_yarfe"]
script/source = "extends Node3D

@onready var player = get_tree().get_first_node_in_group(\"player\")
@onready var next_scene: String = \"res://scenes/Chapter3/Straight/Scene2/Chapter3Scene2CylinderStructure.tscn\"
var quest_file: String = \"res://scenes/quest/resources/follow_the_companion.json\"
func _ready() -> void:
	FadeOutCanvas.fade_in(1)
	Input.set_mouse_mode(Input.MOUSE_MODE_VISIBLE)
	QuestManager.import_quests_from_json(quest_file)
	ItemPopUp.quest_show_message(\"To the Mirror Hall\")

func entering_to_cylinder_structure(body: Node3D) -> void:
	if body == player:
		FadeOutCanvas.fade_out(1.5)
		await get_tree().create_timer(2.0).timeout
		get_tree().change_scene_to_file(next_scene)
"

